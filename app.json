{
  "expo": {
    "splash": {
      "image": "./assets/images/icon.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "name": "HelixNotes",
    "assetBundlePatterns": [
      "./assets/database/*"
    ],
    "slug": "TaskManagerOffline",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "taskmanageroffline",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.emranali15.TaskManagerOffline"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/notes.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/notes.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "23c58ef9-3a36-41f9-9c02-8c29480ae543"
      }
    }
  }
}